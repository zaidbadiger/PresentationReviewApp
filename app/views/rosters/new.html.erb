<h1>Sections page</h1>

<div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @section, local: true) do |f| %>

      <%= render 'shared/error_messages' %>

      <%= f.label :email %>
      <%= f.text_field :email, class: 'form-control' %>
      <%= f.submit "Add Student", class: "btn btn-primary" %>
    <% end %>
  </div>